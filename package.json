{
  "name": "simple-prerender-webpack-plugin",
  "version": "6.0.1",
  "description": "prerender with webpack and html-webpack-plugin",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{js,md}\" \"**/.*rc\"",
    "precommit": "lint-staged",
    "test": "tap \"test/**/*.test.js\" \"test/**/test.js\""
  },
  "author": "dgeibi",
  "license": "MIT",
  "dependencies": {
    "resolve-from": "^4.0.0",
    "source-map": "^0.6.1",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.6.0"
  },
  "devDependencies": {
    "eslint": "^5.6.0",
    "eslint-config-dgeibi": "^5.1.1",
    "fs-extra": "^7.0.0",
    "html-webpack-plugin": "^4.0.0-beta.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.3.0",
    "prettier": "^1.10.2",
    "tap": "^12.0.1",
    "webpack": "^4.20.0"
  },
  "files": [
    "lib/"
  ],
  "peerDependencies": {
    "html-webpack-plugin": "^3.0.0 || ^4.0.0-beta.1",
    "webpack": "^4.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "prettier --write",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ],
    ".*rc": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dgeibi/simple-prerender-webpack-plugin.git"
  },
  "keywords": [
    "webpack"
  ],
  "bugs": {
    "url": "https://github.com/dgeibi/simple-prerender-webpack-plugin/issues"
  },
  "homepage": "https://github.com/dgeibi/simple-prerender-webpack-plugin#readme"
}
